<!DOCTYPE html>
<html lang="en">
<head>
    <style>
        .navbar-expand-sm
        {
            width: 1310px;
        }
        #accordion
        {
            width: 1060px;
        }
        .secondrow
        {
            margin-left: 100px;
        }

        /* #map
        {
            height:500px;
            width:100%;

        } */

    </style>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <script src="https://kit.fontawesome.com/b5d10660ae.js"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
    
    <script>
            $(document).ready(function(){
              $('[data-toggle="popover"]').popover();
              $('[data-toggle="popover"]').popover();
              $('a#desc').on('click', function(e) {e.preventDefault(); return true;});
            });
    </script>
    <title>Document</title>
</head>
<body>
    <div class="row">
        <div class="col-lg-12">
            <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
                <a class="navbar-brand" href="#">&nbsp &nbsp Aparti</a>
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href="#" id="navbardrop" data-toggle="dropdown">
                            Welcome &nbsp <%= user %>
                        </a>
                        
                    </li>
                    
                </ul>
            </nav>
        </div> 
       
    </div>
    <br><br>
    <div class="row">
        <div class="col-lg-5">

        </div>
        <div class="col-lg-7">
        <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal">Flat Owners</button>
            <!-- Modal -->
            <div class="modal fade" id="myModal" role="dialog">
                <div class="modal-dialog">
                <!-- Modal content-->
                    <div class="modal-content">
                        <div class="modal-header">
                            <!-- <button type="button" class="close" data-dismiss="modal">&times;</button> -->
                            <h4 class="modal-title">Header</h4>
                        </div>
                        <div class="modal-body">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Door Number</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%  flatowners.forEach((item, index) => { %>
                                        <tr>
                                            <td><%= item.doornumber %> </td>
                                            <!-- <td>John</td> -->
                                            <!-- <td>Doe</td> -->
                                            <!-- <td>john@example.com</td> -->
                                            <!-- <%= flatowners[0].doornumber %> -->
                                         
                                        </tr>
                                    <% }) %>   
                                </tbody>
                            </table>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    

    




      
</body>
</html>